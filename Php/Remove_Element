    function removeElement(&$nums, $val) {
        
        // Contador de ocorrencias de repetições e o tamanho antes
        $ocorrencias = 0;
        $tamanhoAntes = count($nums);

        // Um looping que atravessa $nums e, caso haja equivalencia, há uma ocorrencia
        for ($i=count($nums)-1; $i>=0; $i--)
        {
            if ($nums[$i] === $val)
            {
                unset($nums[$i]);
                $ocorrencias++;
            }
        }

        return $tamanhoAntes - $ocorrencias;
    }
